{
  "name": "ai-tools-dashboard-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for AI Tools Dashboard static site generation",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0",
    "redis": "^4.6.0",
    "fs-extra": "^11.1.0",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/mustache": "^4.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "private": true
}